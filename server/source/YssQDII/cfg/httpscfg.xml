<?xml version="1.0" encoding="UTF-8"?> 
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:p="http://www.springframework.org/schema/p" 
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"> 
 
	<description>Spring Quick Start</description>
   <bean id="httpsconfig" class="com.yss.main.syssetting.HttpsConfigBean" scope="prototype">
		<property name="httpURL"><!-- 原地址 -->
			<value>http://192.168.16.77:8083/YssQDII</value>
        </property>
		<property name="encrypt"> <!-- 1表示加密 0表示不加密 -->
	        <value>0</value>
	    </property>
	    <property name="httpsURL"><!-- 加密地址 -->
	        <value>https://192.168.16.77:8443/YssQDII</value>
	    </property>
  </bean>

</beans>

